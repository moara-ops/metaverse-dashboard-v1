{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node",
        "config": {
          "distDir": "dist"
        }
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "handle": "filesystem",
        "src": "^/static/(?<path>.*)",
        "headers": {
          "cache-control": "public, max-age=31536000, immutable"
        },
        "dest": "/static/:path"
      },
      {
        "src": "/.*",
        "dest": "/index.html"
      }
    ]
  }
  